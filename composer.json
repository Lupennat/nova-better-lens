{
  "name": "lupennat/nova-better-lens",
  "description": "Laravel Nova - Better Lens",
  "keywords": [
      "laravel",
      "nova"
  ],
  "authors": [
      {
        "name": "Claudio Pennati",
        "email": "claudio.pennati@gmail.com"
      }
    ],
  "license": "MIT",
  "require": {
      "php": "^7.4|^8.0",
      "laravel/nova": ">=4.22.2 <=4.29.5"
  },
  "require-dev": {
      "friendsofphp/php-cs-fixer": "^3.16"
  },
  "autoload": {
      "psr-4": {
          "Lupennat\\BetterLens\\": "src/"
      }
  },
  "extra": {
      "laravel": {
          "providers": [
              "Lupennat\\BetterLens\\BetterLensServiceProvider"
          ]
      }
  },
  "config": {
      "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
  {
      "type": "composer",
      "url": "https://nova.laravel.com"
  }
  ],
  "scripts": {
      "php-cs-fixer": "vendor/bin/php-cs-fixer fix",
      "build": [
          "rm -rf .php-cs-fixer.cache",
          "composer php-cs-fixer src",
          "rm -rf composer.lock",
          "php install.php",
          "npm ci",
          "npm run format",
          "npm run prod"
      ]
  }
}
